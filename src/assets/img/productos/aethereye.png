<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <!-- Fondo gradient azul -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e3a8a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#3b82f6;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="eyeGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e5e7eb;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Fondo -->
  <rect width="100%" height="100%" fill="url(#bgGradient)"/>
  
  <!-- Ojo principal -->
  <ellipse cx="200" cy="150" rx="120" ry="80" fill="url(#eyeGradient)" stroke="#1f2937" stroke-width="3"/>
  
  <!-- Iris -->
  <circle cx="200" cy="150" r="45" fill="#3b82f6"/>
  
  <!-- Pupila -->
  <circle cx="200" cy="150" r="25" fill="#1f2937"/>
  
  <!-- Brillo en el ojo -->
  <ellipse cx="185" cy="135" rx="8" ry="12" fill="#ffffff" opacity="0.8"/>
  
  <!-- Texto AetherEye -->
  <text x="200" y="250" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#ffffff">AetherEye</text>
  
  <!-- Subtítulo -->
  <text x="200" y="275" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#e5e7eb">Productos Químicos</text>
</svg>
